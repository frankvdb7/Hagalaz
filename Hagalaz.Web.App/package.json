{
    "name": "hagalaz.web.app",
    "version": "1.0.0",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=20.7"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:aspire": "ng serve --port %PORT%",
        "build": "ng build --base-href ./",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "launcher:serve": "ng serve --configuration=launcher-development",
        "launcher:build": "ng build --subresource-integrity --configuration=production,launcher-production",
        "launcher:bundle:main": "esbuild src/launcher/main.ts --bundle --platform=node --target=node22.9 --external:electron* --tsconfig=tsconfig.launcher.json --outdir=dist-launcher",
        "launcher:bundle:preload": "esbuild src/launcher/preload.ts --bundle --platform=node --target=node22.9 --external:electron* --tsconfig=tsconfig.launcher.preload.json --outfile=dist-launcher/preload.js",
        "launcher:start": "npm run launcher:bundle:main && npm run launcher:bundle:preload && electron-forge start --enable-logging --app-path dist-launcher/main.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^19.1.3",
        "@angular/cdk": "^19.1.1",
        "@angular/common": "^19.1.3",
        "@angular/compiler": "^19.1.3",
        "@angular/core": "^19.1.3",
        "@angular/forms": "^19.1.3",
        "@angular/material": "^19.1.1",
        "@angular/platform-browser": "^19.1.3",
        "@angular/platform-browser-dynamic": "^19.1.3",
        "@angular/platform-server": "^19.1.3",
        "@angular/router": "^19.1.3",
        "@angular/service-worker": "^19.1.3",
        "@ngrx/operators": "^19.0.0",
        "@ngrx/signals": "^19.0.0",
        "@ngx-loading-bar/core": "^7.0.0",
        "@ngx-loading-bar/http-client": "^7.0.0",
        "@ngx-loading-bar/router": "^7.0.0",
        "angular-svg-icon": "^19.1.1",
        "electron-squirrel-startup": "^1.0.1",
        "inversify": "^6.2.1",
        "ng-hcaptcha": "^2.4.0",
        "overlayscrollbars": "^2.10.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "tslib": "^2.7.0",
        "winston": "^3.17.0",
        "zone.js": "~0.15.0",
        "marked": "^15.0.6"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.1.4",
        "@angular/cli": "^19.1.4",
        "@angular/compiler-cli": "^19.1.3",
        "@angular/language-service": "^19.1.3",
        "@electron-forge/cli": "^7.6.1",
        "@types/jasmine": "~5.1.5",
        "@types/jasminewd2": "^2.0.13",
        "@types/node": "^22.12.0",
        "electron": "^34.0.1",
        "esbuild": "^0.24.2",
        "jasmine-core": "~5.5.0",
        "jasmine-marbles": "^0.9.2",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "tailwindcss": "^3.4.17",
        "typescript": "5.6.3"
    },
    "pnpm": {
        "overrides": {
            "@angular/common": "^19.1.3"
        }
    }
}
