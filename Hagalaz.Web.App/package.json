{
    "name": "hagalaz.web.app",
    "version": "1.0.0",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=20.7"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:aspire": "ng serve --port %PORT%",
        "build": "ng build --base-href ./",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "launcher:serve": "ng serve --configuration=launcher-development",
        "launcher:build": "ng build --subresource-integrity --configuration=production,launcher-production",
        "launcher:bundle:main": "esbuild src/launcher/main.ts --bundle --platform=node --target=node22.9 --external:electron* --tsconfig=tsconfig.launcher.json --outdir=dist-launcher",
        "launcher:bundle:preload": "esbuild src/launcher/preload.ts --bundle --platform=node --target=node22.9 --external:electron* --tsconfig=tsconfig.launcher.preload.json --outfile=dist-launcher/preload.js",
        "launcher:start": "npm run launcher:bundle:main && npm run launcher:bundle:preload && electron-forge start --enable-logging --app-path dist-launcher/main.js"
    },
    "private": true,
    "dependencies": {
        "@angular/cdk": "^19.2.8",
        "@angular/common": "^19.2.5",
        "@angular/compiler": "^19.2.5",
        "@angular/core": "^19.2.5",
        "@angular/forms": "^19.2.5",
        "@angular/material": "^19.2.8",
        "@angular/platform-browser": "^19.2.5",
        "@angular/platform-browser-dynamic": "^19.2.5",
        "@angular/platform-server": "^19.2.5",
        "@angular/router": "^19.2.5",
        "@angular/service-worker": "^19.2.5",
        "@ngrx/operators": "^19.1.0",
        "@ngrx/signals": "^19.1.0",
        "@ngx-loading-bar/core": "^7.0.0",
        "@ngx-loading-bar/http-client": "^7.0.0",
        "@ngx-loading-bar/router": "^7.0.0",
        "angular-svg-icon": "^19.1.1",
        "electron-squirrel-startup": "^1.0.1",
        "inversify": "^7.5.0",
        "ng-hcaptcha": "^2.5.0",
        "overlayscrollbars": "^2.11.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "tslib": "^2.8.1",
        "winston": "^3.17.0",
        "zone.js": "~0.15.0",
        "marked": "^15.0.7"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.2.6",
        "@angular/cli": "^19.2.6",
        "@angular/compiler-cli": "^19.2.5",
        "@angular/language-service": "^19.2.5",
        "@electron-forge/cli": "^7.8.0",
        "@types/jasmine": "~5.1.7",
        "@types/jasminewd2": "^2.0.13",
        "@types/node": "^22.14.0",
        "electron": "^35.1.4",
        "esbuild": "^0.25.2",
        "jasmine-core": "~5.6.0",
        "jasmine-marbles": "^0.9.2",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.8.3"
    },
    "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af"
}
