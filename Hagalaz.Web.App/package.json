{
    "name": "hagalaz.web.app",
    "version": "1.0.0",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=20.7"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:aspire": "ng serve --port %PORT%",
        "build": "ng build --base-href ./",
        "test": "ng test --watch=false",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "launcher:serve": "ng serve --configuration=launcher-development",
        "launcher:build": "ng build --subresource-integrity --configuration=production,launcher-production",
        "launcher:bundle:main": "esbuild src/launcher/main.ts --bundle --platform=node --target=node22.9 --external:electron* --tsconfig=tsconfig.launcher.json --outdir=dist-launcher",
        "launcher:bundle:preload": "esbuild src/launcher/preload.ts --bundle --platform=node --target=node22.9 --external:electron* --tsconfig=tsconfig.launcher.preload.json --outfile=dist-launcher/preload.js",
        "launcher:start": "npm run launcher:bundle:main && npm run launcher:bundle:preload && electron-forge start --enable-logging --app-path dist-launcher/main.js"
    },
    "private": true,
    "dependencies": {
        "@angular/cdk": "^20.2.11",
        "@angular/common": "^20.3.9",
        "@angular/compiler": "^20.3.9",
        "@angular/core": "^20.3.9",
        "@angular/forms": "^20.3.9",
        "@angular/material": "^20.2.11",
        "@angular/platform-browser": "^20.3.9",
        "@angular/platform-browser-dynamic": "^20.3.9",
        "@angular/platform-server": "^20.3.9",
        "@angular/router": "^20.3.9",
        "@angular/service-worker": "^20.3.9",
        "@ngrx/operators": "^20.1.0",
        "@ngrx/signals": "^20.1.0",
        "@ngx-loading-bar/core": "^7.0.0",
        "@ngx-loading-bar/http-client": "^7.0.0",
        "@ngx-loading-bar/router": "^7.0.0",
        "angular-svg-icon": "^19.1.1",
        "electron-squirrel-startup": "^1.0.1",
        "inversify": "^7.10.2",
        "marked": "^16.4.1",
        "ng-hcaptcha": "^2.6.0",
        "overlayscrollbars": "^2.12.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "tslib": "^2.8.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@angular/build": "^20.3.8",
        "@angular-devkit/build-angular": "^20.3.8",
        "@angular/cli": "^20.3.8",
        "@angular/compiler-cli": "^20.3.9",
        "@angular/language-service": "^20.3.9",
        "@electron-forge/cli": "^7.10.2",
        "@tailwindcss/postcss": "^4.1.16",
        "@types/node": "^24.0.15",
        "electron": "^39.0.0",
        "esbuild": "^0.25.12",
        "jsdom": "^27.1.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "tailwindcss": "^4.1.16",
        "typescript": "~5.8.3",
        "vitest": "^3.2.4"
    },
    "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
