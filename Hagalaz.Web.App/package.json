{
    "name": "hagalaz.web.app",
    "version": "1.0.0",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=20.7"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:aspire": "ng serve --port %PORT%",
        "build": "ng build --base-href ./",
        "test": "ng test --watch=false",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "launcher:serve": "ng serve --configuration=launcher-development",
        "launcher:build": "ng build --subresource-integrity --configuration=production,launcher-production",
        "launcher:bundle:main": "esbuild src/launcher/main.ts --bundle --platform=node --target=node22.9 --external:electron* --tsconfig=tsconfig.launcher.json --outdir=dist-launcher",
        "launcher:bundle:preload": "esbuild src/launcher/preload.ts --bundle --platform=node --target=node22.9 --external:electron* --tsconfig=tsconfig.launcher.preload.json --outfile=dist-launcher/preload.js",
        "launcher:start": "npm run launcher:bundle:main && npm run launcher:bundle:preload && electron-forge start --enable-logging --app-path dist-launcher/main.js"
    },
    "private": true,
    "dependencies": {
        "@angular/cdk": "^20.1.2",
        "@angular/common": "^20.1.2",
        "@angular/compiler": "^20.1.2",
        "@angular/core": "^20.1.2",
        "@angular/forms": "^20.1.2",
        "@angular/material": "^20.1.2",
        "@angular/platform-browser": "^20.1.2",
        "@angular/platform-browser-dynamic": "^20.1.2",
        "@angular/platform-server": "^20.1.2",
        "@angular/router": "^20.1.2",
        "@angular/service-worker": "^20.1.2",
        "@ngrx/operators": "^19.2.1",
        "@ngrx/signals": "^19.2.1",
        "@ngx-loading-bar/core": "^7.0.0",
        "@ngx-loading-bar/http-client": "^7.0.0",
        "@ngx-loading-bar/router": "^7.0.0",
        "angular-svg-icon": "^19.1.1",
        "electron-squirrel-startup": "^1.0.1",
        "inversify": "^7.6.1",
        "marked": "^16.1.1",
        "ng-hcaptcha": "^2.6.0",
        "overlayscrollbars": "^2.11.4",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "tslib": "^2.8.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@angular/build": "^20.1.1",
        "@angular-devkit/build-angular": "^20.1.1",
        "@angular/cli": "^20.1.1",
        "@angular/compiler-cli": "^20.1.2",
        "@angular/language-service": "^20.1.2",
        "@electron-forge/cli": "^7.8.1",
        "@tailwindcss/postcss": "^4.1.11",
        "@types/node": "^24.0.15",
        "electron": "^37.3.1",
        "esbuild": "^0.25.8",
        "jsdom": "^26.1.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^4.1.11",
        "typescript": "~5.8.3",
        "vitest": "^3.2.4"
    },
    "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
